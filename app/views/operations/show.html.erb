
<% if is_representative? || is_admin? %>
	<div class="container">
		<br>
		<br>
		<nav class="breadcrumb" aria-label="breadcrumbs">
		  <ul>
		    <li class='operations-redirection' ><%= link_to "Operation Records List", is_admin? ? general_records_path : representative_path %></li>
		    <li class="is-active"><%= link_to "Operation Number " + @operation.id.to_s, operation_path(@operation.id) %></li>
		  </ul>
		</nav>
	</div>
	<br>
	<%= render 'timeline', operation_id: @operation.id, agent: @agent, shipper: @shipper, tasks: @tasks, cargo_info_tasks: @cargo_info_tasks, 
	info_confirmed_tasks: @info_confirmed_tasks, info_requested_tasks: @info_requested_tasks, quotation_confirmed_tasks: @quotation_confirmed_tasks %>
<% elsif is_agent? %>
<% end %>

