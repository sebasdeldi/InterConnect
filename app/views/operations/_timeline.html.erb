<br>
<br>
<div class="container is-widescreen"> 
	<% if is_fcl_exw? operation_id %>
		<%= render 'operations/fcl_exw/fcl_exw_timeline', operation_secure_id: operation_secure_id, operation_id: operation_id, agent: agent, shipper: shipper, fcl_exw_container_loading_tasks: fcl_exw_container_loading_tasks , carriers: carriers, consignee: consignee, info_requested_tasks: info_requested_tasks, quotation_confirmed_tasks: quotation_confirmed_tasks, info_confirmed_tasks: info_confirmed_tasks, cargo_info_tasks: cargo_info_tasks, request_booking_tasks: request_booking_tasks, carriers: carriers, booking_info_tasks: booking_info_tasks, 	 fcl_exw_container_delivery_tasks: fcl_exw_container_delivery_tasks, fcl_exw_container_loading_tasks: fcl_exw_container_loading_tasks %>
	<% else %>
		<%= render 'other_timeline', operation_id: operation_id, agent: agent, shipper: shipper %>
	<% end %>

	<%= render 'relevant_info_modal', agent: agent, shipper: shipper, consignee: consignee %>

	<!-- Tasks modals -->
	<%= render 'generals/tasks/fcl_exw/cargo_info_step', agent: agent, shipper: shipper, operation_id: operation_id %>
	<%= render 'generals/tasks/fcl_exw/booking_info_step', agent: agent, shipper: shipper, operation_id: operation_id %>
	<%= render 'generals/tasks/fcl_exw/info_confirmed_step', agent: agent, shipper: shipper, operation_id: operation_id %>
	<%= render 'generals/tasks/fcl_exw/info_requested_step', agent: agent, shipper: shipper, operation_id: operation_id %>
	<%= render 'generals/tasks/fcl_exw/quotation_confirmed_step', agent: agent, shipper: shipper, operation_id: operation_id %>
	<%= render 'generals/tasks/fcl_exw/request_booking_step', agent: agent, shipper: shipper, operation_id: operation_id %>
	<%= render 'generals/tasks/fcl_exw/loading_confirmed_step', agent: agent, shipper: shipper, operation_id: operation_id %>
	<%= render 'generals/tasks/fcl_exw/delivery_booking_step', agent: agent, shipper: shipper, operation_id: operation_id %>
	<!-- Steps modals -->


	<!-- Hidding this on purpose -->
	<% render 'send_follow_up_mail_modal', agent: agent, shipper: shipper, operation_id: operation_id %>

	<%= link_to edit_operations_by_user_path(operation_id) do %>
		<i class="fas fa-cog middle-icon settings-button show-modal"></i>
	<% end %>

	<a class="show-modal relevant-info">
	  <i class="fas fa-info-circle middle-icon fixed-bottom-button show-modal" ></i>
	</a>
</div>

