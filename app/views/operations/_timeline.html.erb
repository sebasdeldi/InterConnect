
<div class='timeline'>
	<div class="container">
    <h1 class="project-name">OPERATION <b><%= operation_id %></b> FOR <b><%= agent.company_name %></b> </h1>
		<div id="timeline">
			<% if is_fcl_exw? operation_id %>
				<%= render 'fcl_exw_info_timeline_item', operation_id: operation_id, agent: agent %>
				<%= render 'timeline_item_example1' %>
				<%= render 'timeline_item_example2' %>
			<% else %>
				<%= render 'general_cargo_info_timeline_item', operation_id: operation_id, agent: agent %>
				<%= render 'timeline_item_example1' %>
				<%= render 'timeline_item_example2' %>
			<% end %>
		</div>
	</div>	
</div>
	